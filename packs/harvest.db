{"name":"Crocodile Teeth (pouch)","type":"loot","sort":100001,"img":"icons/containers/bags/coinpouch-simple-leather-brown.webp","effects":[],"system":{"description":{"value":"A successful harvest can yield over 60 teeth, the longest being over 4 inches long. Crocodile teeth can be used to create simple jewelry and crafts.","chat":"","unidentified":""},"source":"Crocodile","quantity":1,"weight":2,"price":{"value":0.3,"denomination":"gp"},"attunement":0,"equipped":false,"rarity":"","identified":true},"folder":null,"ownership":{"default":0,"UDIYq77k2aTlndBT":3},"flags":{},"_stats":{"systemId":"dnd5e","systemVersion":"2.1.5","coreVersion":"10.291","createdTime":1687316113854,"modifiedTime":1687316113854,"lastModifiedBy":"UDIYq77k2aTlndBT"},"_id":"uofDCsu6oQYmqWoK"}
